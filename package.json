{
  "name": "idle-roman-game",
  "description": "A TypeScript project template that demonstrates React communication and uses Vite for bundling.",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joseorono/idle-roman-game.git"
  },
  "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/joseorono/idle-roman-game/issues"
  },
  "homepage": "https://github.com/joseorono/idle-roman-game#readme",
  "keywords": [
    "react",
    "vite",
    "typescript"
  ],
  "scripts": {
    "test-cli": "vitest --run --reporter verbose",
    "test": "vitest --ui",
    "test-filter": "vitest --reporter verbose --testNamePattern",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier-format": "prettier --config .prettierrc --write src/**/*.ts src/**/*.tsx spec/**/*.ts",
    "prettier-watch": "onchange src/**/*.ts src/**/*.tsx spec/**/*.ts -- prettier --write {{changed}}",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "visualize-bundle": "vite-bundle-visualizer & vite-bundle-visualizer -t sunburst & vite-bundle-visualizer -t network",
    "check-updates": "ncu",
    "update-deps": "ncu -u && npm install"
  },
  "dependencies": {
    "@pixi/react": "^7.1.2",
    "@react-spring/web": "^9.7.3",
    "@use-gesture/react": "^10.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "howler": "^2.2.4",
    "localforage": "^1.10.0",
    "lucide-react": "^0.378.0",
    "pixi.js": "^8.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-fast-marquee": "^1.6.4",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.23.1",
    "simple-zustand-devtools": "^1.1.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "timeline-composer": "^0.1.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "^15.0.7",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.11.1",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.0.11",
    "npm-check-updates": "^16.14.20",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rollup-plugin-visualizer": "^5.12.0",
    "sharp": "^0.33.4",
    "simple-zustand-devtools": "^1.1.0",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.3",
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
